package MySever;

import java.io.IOException;
import java.io.InputStream;
import java.net.ServerSocket;
import java.net.Socket;

public class Sever2 {
	private ServerSocket sever;

	public void start(){
		try {
			sever = new ServerSocket(8888);
			recive();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
	public void recive(){
		try {
			Socket client = sever.accept();
			byte[] data = new byte[20480];
			InputStream is =client.getInputStream();
			int len = is.read(data);
			String msg=new String(data,0,len);
			
			System.out.println(msg.trim());
			
		} catch (IOException e) {
			e.printStackTrace();
		}
		
	}
	public void stop(){
		
	}
	public static void main(String[] args) {
		Sever2 sever = new Sever2();
		sever.start();
	}

}


class Responds{
	private StringBuilder stateline;
	private StringBuilder ResponseHeader;
	private StringBuilder contents;
	
	public Responds(){
		this.stateline = new StringBuilder();
		ResponseHeader = new StringBuilder();
		this.contents = new StringBuilder();
		
	}
	public void setState(){
		
	}
}
