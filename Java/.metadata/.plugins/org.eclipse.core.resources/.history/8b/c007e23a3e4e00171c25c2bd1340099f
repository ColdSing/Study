package synchronize;

public class SynTest {

	public static void main(String[] args) {
		
	}

}

class Web12306 implements Runnable{
	private int num = 10;
	private boolean flag = true;
	@Override
	public void run() {
		while(flag){
			if(num<1){
				flag=false;
			}
			System.out.println(Thread.currentThread().getName()+"抢到了第"+num--+"张票");
		}
	}
	
}