class Cell{
	private boolean isalive;
	private int column;//┴л
	private int row;//лл
	public Cell(int row,int column,boolean isalive) {
		this.row=row;
		this.column=column;
		this.isalive=isalive;
	}
	public void die(){
		this.isalive=false;
	}
	public void reborn(){
		this.isalive=true;
	}
}

