class Cell{
	private boolean isalive;
	
	public Cell(boolean isalive) {
		this.isalive=isalive;
	}
	public void die(){
		this.isalive=false;
	}
	public void reborn(){
		this.isalive=true;
	}
}

class CellGrids{
	private int columns;//┴л
	private int rows;//лл
	
}