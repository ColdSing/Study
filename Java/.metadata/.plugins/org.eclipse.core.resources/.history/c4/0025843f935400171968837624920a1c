package wrapclass;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int columns=in.nextInt();
        int rows = in.nextInt();
        CellGrids cg = new CellGrids(columns, rows);
    	int row = in.nextInt();
    	int column=in.nextInt();
    	while(row!=-1){
    		cg.setAlive(row, column);
    		row = in.nextInt();
    		column=in.nextInt();
    	}
        
        in.close();
    }
}

class Cell{
	private boolean isalive;
	private boolean before;
	
	public Cell(boolean isalive) {
		this.isalive=isalive;
		this.before=isalive;
	}
	public void die(){
		this.before=this.isalive;
		this.isalive=false;
	}
	public void reborn(){
		this.before=this.isalive;
		this.isalive=true;
	}
	public boolean getBefore(){
		return before;
	}
	public boolean getAlive(){
		return isalive;
	}
	public void initialAlive(){
		this.isalive = true;
		this.before=true;
	}
}

class CellGrids{
	private int columns;//┴л
	private int rows;//лл
	public static final boolean GOOD = true;
	public static final boolean BAD = false;
	Cell[][] cells;
	public CellGrids(int columns, int rows) {
		this.columns = columns;
		this.rows = rows;
		this.cells = new Cell[rows][columns];
		cellInitial();
	}
	private  void cellInitial(){
		for(int i=0;i<rows;i++){
			for(int j=0;j<columns;j++){
				cells[i][j]=new Cell(false);
			}
		}
	}
	public void setAlive(int row,int column){
		cells[row][column].initialAlive();
	}
	public void start(){
		for(int i=0;i<rows;i++){
			for(int j=0;j<columns;j++){
				if(checkAround(i,j)){
					cells[i][j].reborn();
				}else{
					cells[i][j].die();;
				}
			}
		}
	}
	public boolean checkAround(int row,int column){
		int sum=0;
		for(int i=row-1;i<=row+1;i++){
			for(int j=column-1;j<=column+1;j++){
				if(i>=0&&j>=0&&i<rows&&j<columns&&!(i==row&&j==column)){
					if(cells[i][j].getBefore()){
						sum++;
					}
				}
			}
		}
		if(2==sum||3==sum){
			return GOOD;
		}else{
			return BAD;
		}
	}
	public int countAlive(){
		int sum=0;
		for(int i=0;i<rows;i++){
			for(int j=0;j<columns;j++){
				if(cells[i][j].getAlive()){
					sum++;
				}
			}
		}
		return sum;
	}
}